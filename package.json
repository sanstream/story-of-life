{
  "name": "pixi-webpack-demo",
  "version": "1.0.0",
  "description": "make pixi.js game with webpack",
  "main": "src/js/main.js",
  "keywords": [
    "pixi.js",
    "webpack"
  ],
  "author": "sanstream",
  "scripts": {
    "dev": "cross-env NODE_ENV=development  webpack-dev-server --config webpack.dev.js",
    "clean": "rimraf dist",
    "lint": "eslint ./",
    "test:ui": "cypress open",
    "prepare": "install-peers",
    "ts-build": "./node_modules/typescript/bin/tsc",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "@bankai/eslint-config": "^0.2.2",
    "canvas-sketch-util": "^1.10.0",
    "core-js": "^3.6.4",
    "eslint": "^7.18.0",
    "pixi.js": "^5.2.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "cypress": "^6.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "install-peers-cli": "^2.2.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.18",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
